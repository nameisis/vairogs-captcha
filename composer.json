{
    "name": "vairogs/vairogs-captcha",
    "description": "Real Latvian CMS Utils: Captcha library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "vairogs",
        "captcha",
        "symfony"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "vairogs/vairogs-core": "dev-master",
        "symfony/dependency-injection": "*",
        "symfony/http-kernel": "*",
        "symfony/config": "*",
        "symfony/http-foundation": "*",
        "symfony/form": "*",
        "symfony/options-resolver": "*",
        "symfony/validator": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Utils\\Captcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vairogs\\Utils\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "4.1.*"
        }
    }
}
